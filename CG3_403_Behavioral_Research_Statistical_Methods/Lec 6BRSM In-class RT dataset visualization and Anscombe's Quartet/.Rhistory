plot(trials, mean_trial_A, type="l", col="blue",
xlab="Trial", ylab="Reaction Time (ms)",
main="Learning Curve Across Trials")
lines(trials, mean_trial_B, col="red")
legend("topright", legend=c("A","B"), col=c("blue","red"), lty=1)
